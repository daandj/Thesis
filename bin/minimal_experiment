#!/usr/bin/env python

import sys
import numpy as np

import cbt.games.minimal as mg

def main() -> int:
    means = np.random.random_sample((5,5))

    print("Means")
    print(means)

    print("Results")
    game = mg.Minimal(means=means)
    first_player = mg.CBTMinimalPlayer(0, data_flag=True)
    first_player.set_iteration(1000)
    first_player.make_move(game)
    return 0

if __name__ == '__main__':
    sys.exit(main())
